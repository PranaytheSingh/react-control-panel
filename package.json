    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.21.4",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "standard": "^14.3.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  }
